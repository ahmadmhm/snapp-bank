{
    "name": "ahmad/snapp",
    "type": "project",
    "authors": [
        {
            "name": "Ahmad Mohammadi"
        }
    ],
    "description": "The First Technical Task.",
    "keywords": ["php", "snap"],
    "license": "Myself",
    "require": {
        "php": "^8.3",
        "laravel/pint": "^1.20",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
